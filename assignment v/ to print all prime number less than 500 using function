// to print all prime number less than 500 using function
#include <stdio.h>
#include <conio.h>

void prime(){
	int i,j,count;
	
	for(i=1;i<500;i++){
		count=0;
		
		for(j=1;j<=i;j++){
			if(i%j==0){
				count++;
			}
		}
		
		if(count==2){
			printf("%d\t",i);
   }
		
	}
}

int main(){
	printf("Prime NUmbers between 0 to 500\t");
	
	prime();
    getch();	
}
